\n        return formattedTable;\r\n    }\r\n\r\n    function generateTableEntry(data) {\r\n        let columns = [];\r\n        const mapping = {\r\n            \"Base Stats\": addStats,\r\n            \"Typing\": addTyping,\r\n            \"Abilities\": addAbilities\r\n        }\r\n\r\n        for (let label in mapping) {\r\n            props.display[label] && columns.push(mapping[label](data));\r\n        }\r\n\r\n        return (\r\n            <tr key={data.id}>\r\n                {addLabel(data)}\r\n                {columns}\r\n            </tr>\r\n        )\r\n    }\r\n\r\n    function addLabel(data) {\r\n        return (\r\n            <td className=\"d-flex flex-column justify-content-center align-items-center\"\r\n                key={data.id}>\r\n                <img\r\n                    src={data.sprites.other[\"official-artwork\"].front_default}\r\n                    className=\"icon\"\r\n                    alt={\"Image of a \" + data.name} />\r\n                <p>{capitalize(data.name)}</p>\r\n            </td>\r\n        )\r\n    }\r\n\r\n    function addStats(data) {\r\n        let columns = [];\r\n        data.stats.forEach((stat, index) => \r\n            columns.push(\r\n                <td className=\"align-middle\"\r\n                    key={data.id + \"\" + index}>\r\n                    {stat.base_stat}\r\n                </td>\r\n            )\r\n        );\r\n        return columns;\r\n    }\r\n\r\n    function addTyping(data) {\r\n        let columns = [];\r\n        data.types.forEach(typeObj => columns.push(\r\n            <td key={data.id  + \"-\"+ typeObj.type.name}>\r\n                <img src={\"https://www.serebii.net/pokedex-bw/type/\" + typeObj.type.name + \".gif\"} />\r\n            </td>)\r\n        );\r\n\r\n        columns = addUpTo(columns, 2);\r\n        return columns;\r\n    }\r\n\r\n    function addAbilities(data) {\r\n        let columns = [];\r\n        data.abilities.forEach(abilityObj =>\r\n            columns.push(\r\n            <td key={data.id + abilityObj.ability.name}>\r\n                {cleanAbility(abilityObj.ability.name)}\r\n            </td>)\r\n        )\r\n\r\n        columns = addUpTo(columns, 4);\r\n        return columns;\r\n    }\r\n\r\n    function addUpTo(columns, limit) {\r\n        while (columns.length < limit) {\r\n            columns.push(<td key={columns.length} />)\r\n        }\r\n        return columns\r\n    }\r\n\r\n    function cleanAbility(string) {\r\n        return capitalize(string.replace('-', ' '));\r\n    }\r\n    \r\n    function capitalize(string) {\r\n        return string.replace(/(\\b[a-z](?!\\s))/g, (c) => c.toUpperCase());\r\n    }\r\n\r\n    return (\r\n        <tbody>\r\n            {generateTable(props.tableData)}\r\n        </tbody>\r\n    )\r\n}\r\n\r\nexport default TableBody;","C:\\Users\\isaacrez\\Documents\\NetBeansProjects\\PokeDraftLeague\\client\\src\\components\\pokemonTab\\TableHeader.js",[],"C:\\Users\\isaacrez\\Documents\\NetBeansProjects\\PokeDraftLeague\\client\\src\\components\\pokemonTab\\PokeTable.js",[],"C:\\Users\\isaacrez\\Documents\\NetBeansProjects\\PokeDraftLeague\\client\\src\\components\\pokemonTab\\Pokemon.js",[],"C:\\Users\\isaacrez\\Documents\\NetBeansProjects\\PokeDraftLeague\\client\\src\\components\\pokemonTab\\PokeSearchBar.js",[],"C:\\Users\\isaacrez\\Documents\\NetBeansProjects\\PokeDraftLeague\\client\\src\\components\\general\\TableNavBar.js",[],"C:\\Users\\isaacrez\\Documents\\NetBeansProjects\\PokeDraftLeague\\client\\src\\components\\general\\Header.js",[],"C:\\Users\\isaacrez\\Documents\\NetBeansProjects\\PokeDraftLeague\\client\\src\\components\\homeTab\\Home.js",[],"C:\\Users\\isaacrez\\Documents\\NetBeansProjects\\PokeDraftLeague\\client\\src\\components\\homeTab\\LeagueSelector.js",[],"C:\\Users\\isaacrez\\Documents\\NetBeansProjects\\PokeDraftLeague\\client\\src\\components\\general\\ToggleBar.js",[],"C:\\Users\\isaacrez\\Documents\\NetBeansProjects\\PokeDraftLeague\\client\\src\\c